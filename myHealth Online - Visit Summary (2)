.incLoad { height: 100px; border: 1px dashed rgb(204, 204, 204); color: rgb(102, 102, 102); font: 9pt Arial; cursor: wait; padding: 4px; margin: 10px 0px; }

.rpt .cardColumn .pgSection[data-sectdepth="1"] > .sectionHeader .title, .rpt .mainColumn .pgSection[data-sectdepth="1"] > .sectionHeader .title, .rpt .docHeader .title { color: rgb(10, 100, 156); }

.rpt .sectHlt13 .pgSection .sectionHeader .title, .rpt .sectHlt14 .pgSection .sectionHeader .title, .rpt .sectHlt15 .pgSection .sectionHeader .title { color: rgb(0, 0, 0); }

