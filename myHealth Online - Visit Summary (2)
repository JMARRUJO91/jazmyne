#proxies .proxy.preview, #proxies .proxy.selected:not(.shrink) { font-weight: bold; width: 3.5em; z-index: 10; }

#proxies .proxy a { display: block; height: calc(100% - 0.25rem); padding: 0.5em 0px 0px; }

#proxies .proxy.preview > a, #proxies .proxy.selected:not(.shrink) > a { padding: 0px; }

